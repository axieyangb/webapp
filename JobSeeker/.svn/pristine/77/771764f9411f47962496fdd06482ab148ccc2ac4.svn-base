.header {
    position: absolute;
    left: 0;
    width: 100%;
    padding-top: 15px;
    padding-bottom: 15px;
    margin-left: -15px;
    margin-right: -15px;
    margin: 0;
    float: right;
}

.brand {
    float: left;
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 20px;
    font-weight: 500;
    white-space: nowrap;
    margin: auto 0;
    line-height: 48px;
    color: white;
    z-index: 200;
}

.signinBtn {
    float: inherit;
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 0;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    margin-right: 10PX;
}

.showMore {
    float: inherit;
    cursor: pointer;
    margin: 10px 30px 10px 10px;
    align-items: center;
}

.profile-img {
    float: inherit;
    margin: 0px 30px 0px 10px;
    height: inherit;
    width: 40px;
}

.profile-img>img {
    float: inherit;
    width: inherit;
    height: inherit;
    -moz-border-radius: 50%;
    -webkit-border-radius: 50%;
    border-radius: 50%;
    align-items: center;
}

.dropdown {
    float: inherit;
    z-index: 999;
}

.dropdown-item {
    display: block;
    width: 100%;
    padding: 3px 1.5rem;
    font-weight: 400;
    text-align: inherit;
    white-space: nowrap;
    background: 0 0;
    border: 0;
}

.dropdown-profile {
    position: absolute;
    top: 100%;
    left: -130;
    z-index: 1000;
    float: left;
    background-color: #fff;
    color: #333;
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
    border: 1px solid rgba(0, 0, 0, .15);
}

.dropdown-more {
    position: absolute;
    width: 254px;
    top: 100%;
    left: -250px;
    padding: 5px;
    z-index: 1000;
    float: left;
    background-color: #fff;
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
    border: 1px solid rgba(0, 0, 0, .15);
}

.cursor-pointer {
    cursor: pointer;
}

.oneMoreItem {
    width: 80px;
    padding: 0;
    height: 80px;
    /*margin: -1px;*/
    float: left;
    border: 1px solid transparent;
}

.oneMoreItem:hover {
    border: solid 1px #d0d0d0;
}

.dropdown-profile>li:hover {
    background-color: #e6e6e6;
    border-color: #adadad;
}


